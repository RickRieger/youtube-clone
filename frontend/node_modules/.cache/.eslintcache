eoId}`);\n  };\n\n  return (\n    <div\n      className='videoContainer'\n      onClick={() => handleOnClick(props.video_id)}\n    >\n      <img className='videoThumbnail' src={props.image}></img>\n      <div className='videoContainerInfo'>\n        <div className='videoContainerText'>\n          <p className='videoTitleText'>{props.title}</p>\n          <p className='videoViewsText'>\n            {' '}\n            {props.channel}\n            <br></br> views: {props.views} ·{' '}\n            {moment(props.uploadDate).startOf('day').fromNow()}\n          </p>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Video;\n",{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":15,"column":8,"nodeType":"107","messageId":"108","endLine":15,"endColumn":20},{"ruleId":"109","severity":1,"message":"110","line":19,"column":6,"nodeType":"111","endLine":19,"endColumn":21,"suggestions":"112"},{"ruleId":"105","severity":1,"message":"113","line":12,"column":10,"nodeType":"107","messageId":"108","endLine":12,"endColumn":14},{"ruleId":"105","severity":1,"message":"114","line":13,"column":10,"nodeType":"107","messageId":"108","endLine":13,"endColumn":14},{"ruleId":"105","severity":1,"message":"115","line":12,"column":8,"nodeType":"107","messageId":"108","endLine":12,"endColumn":25},{"ruleId":"116","severity":1,"message":"117","line":26,"column":11,"nodeType":"118","endLine":30,"endColumn":13},{"ruleId":"109","severity":1,"message":"119","line":13,"column":6,"nodeType":"111","endLine":13,"endColumn":8,"suggestions":"120"},{"ruleId":"105","severity":1,"message":"121","line":8,"column":9,"nodeType":"107","messageId":"108","endLine":8,"endColumn":17},{"ruleId":"122","severity":1,"message":"123","line":17,"column":7,"nodeType":"118","endLine":17,"endColumn":57},"no-native-reassign",["124"],"no-negated-in-lhs",["125"],"no-unused-vars","'PrivateRoute' is defined but never used.","Identifier","unusedVar"